{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
"plugin:@typescript-eslint/recommended",
"prettier/@typescript-eslint",
"plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
"prettier"
    ],
    "rules": {
	   	"import/extensions": [
	      "error",
"prettier/prettier": "error",
	      "ignorePackages",
	      {
	        "ts": "never"
	      }
	    ]
	  },
	  "settings": {
	    "import/resolver": {
	      "typescript": {}
	    }
	  } 
}
